<div class="page-header">
  <h1>Listing logged in users</h1>
</div>
<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <%= content_tag_for(:tr, @logged_in_users) do |user| %>
        <td><%= user.email %></td>
      <% end %>
    </tbody>
  </table>
</div>